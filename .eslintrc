{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "id-length": [0],
    "no-use-before-define": [0],
    "object-curly-spacing": [0],
    "no-param-reassign": [0],
    "react/jsx-no-bind": [0],
    "react/no-multi-comp": [0],
    "react/prop-types": [1, { "ignore": ["dispatch", "children"]}]
  }
}
